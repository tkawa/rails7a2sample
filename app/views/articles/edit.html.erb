<h1>Editing article</h1>

<turbo-frame id="<%= dom_id @article %>">
<%= render "form", article: @article %>
<%= link_to "Cancel", @article, "data-turbo-action": "restore" %>
</turbo-frame>

<br>

<div>
  <%= link_to "Show this article", @article %> |
  <%= link_to "Back to articles", articles_path %>
</div>
